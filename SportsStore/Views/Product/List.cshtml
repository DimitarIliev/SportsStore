@model ProductsListViewModel

@foreach (var item in Model.Products)
{
    @Html.Partial("_ProductSummary", item)
}

<div page-model="@Model.PagingInfo" page-action="List" page-classes-enabled="true" page-class="btn" page-class-normal="btn-default"
page-class-selected="btn-primary" page-url-category="@Model.CurrentCategory" class="btn-gruop pull-right"></div>